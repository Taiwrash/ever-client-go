STGRAMS
PUSHINT 105
STZEROES
STSLICECONST 0
STSLICECONST 0
ENDC
PUSHINT 0
SENDRAWMSG
; end function sendTransaction
BLKDROP 3
; end modifier checkOwnerAndAccept
EQINT -1
PUSHCONT {
	CALL $c7_to_c4$
}
IF
TRUE
